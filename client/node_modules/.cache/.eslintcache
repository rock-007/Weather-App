ubmit={handleSubmit}>\n                <input\n                    type=\"text\"\n                    name=\"city\"\n                    id=\"city\"\n                    placeholder=\"City\"\n                />\n                <button id=\"search-button\" type=\"submit\">Search</button>\n                <input type=\"submit\" value=\"Save\" id=\"save\" />\n            </form>\n        </div>\n    );\n};\n\n\nexport default SearchForm;","/Users/wellingtonblack/codeclan_work/React_Weather_App_Project/weather-app/client/src/components/DailyForecast.js",[],"/Users/wellingtonblack/codeclan_work/React_Weather_App_Project/weather-app/client/src/components/WeeklyForecast.js",["80","81","82","83"],"import {WiThermometer, WiStrongWind, WiDaySunny, WiRain, WiDayShowers, WiDayCloudy} from \"react-icons/wi\";\n\nconst WeeklyForecast = ({main, wind, clouds, rain, day}) => {\n\n    let displayClouds = function (clouds, rain) {\n\n        let rainCount = getRainCount(rain)\n        if (clouds.all > 40 && rainCount > 4) {\n            return <WiDayShowers class=\"icon\" />\n        } else if (clouds.all < 40) {\n            return <WiDaySunny class=\"icon\" />\n        } else {\n            return <WiDayCloudy class=\"icon\" />\n        }\n    }\n\n    let getRainCount = function (rain) {\n        let rainCount = 0\n        for (let rainNumber in rain)\n            if (rainNumber == '3h')\n                rainCount = rain['3h']\n            else if (rainNumber == '1h')\n                rainCount = rain['1h']\n        return rainCount\n    }\n\n    return (\n        <div id=\"day-forecast\">\n            <h4 id=\"day-number\"> {\n                day == 0 ? 'Today'\n                    : day == 1 ? 'Tomorrow'\n                        : `Day ${day + 1}`}\n            </h4>\n            <ul id=\"details-list\">\n                <li id= \"sunny-cloudy\">\n                    {displayClouds(clouds, rain)}\n                </li>\n                <li id=\"temperature\">\n                  