\n            })\n            .then((result) =>\n                setCities([...cities, {daily: result[0], forecast: result[1]}])\n            );\n    };\n\n    const onCitySubmit = function (city) {\n        setSelectedCity(city);\n    };\n\n    return (\n        <div>\n            <SearchForm cities={cities} onCitySubmit={onCitySubmit} />\n            {selectedCity != null ? <City cities={cities} /> : null}\n        </div>\n    );\n};\n\nexport default WeatherContainer;\n","/Users/lina/codeclan_work/week_08/weather-app/client/src/components/SearchForm.js",[],"/Users/lina/codeclan_work/week_08/weather-app/client/src/components/City.js",["44"],"import DailyForecast from \"./DailyForecast\";\nimport WeeklyForecast from \"./WeeklyForecast\";\nimport './City.css';\n//const city = {daily:null, forcast:null}\n\nconst City = ({cities}) => {\n    // const eachCity = cities.map((city, index) => {\n    //     console.log(city);\n    //     return <DailyForcast city={city.daily} key={index} />;\n    // });\n    const cityforecast = cities.map((city) => {\n        // console.log(city);\n\n        return (\n            <div class=\"city-card\">\n                <h3 id=\"city-name\">{city.daily.name}</h3>\n                {city.forecast.list\n                    .map((eachDay, index) => (\n                        <WeeklyForecast\n                            main={eachDay.main}\n                            visibility={eachDay.visibility}\n                            wind={eachDay.wind}\n                            day={index}\n                            clouds={eachDay.clouds[\"all\"]}\n                            rain={eachDay.rain}\n                            key={index}\n                        />\n                    ))\n                    .splice(0, 7)}\n            </div>\n        );\n    });\n    return (\n        <div>\n            <ul>{cityforecast}</ul>\n        </div>\n    );\n};\n\nexport default City;\n","/Users/lina/codeclan_work/week_08/weather-app/client/src/com